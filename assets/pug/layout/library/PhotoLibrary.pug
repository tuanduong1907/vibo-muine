section.libraryWrapper.container 
    +render_tripAdvisor_traveler()
    .photoTabListHeader 
        .libraryTabItem.active(data-tab= '1') All
        .libraryTabItem(data-tab='2') Overview
        .libraryTabItem(data-tab='3') Room
        .libraryTabItem(data-tab='4') Dinning
        .libraryTabItem(data-tab='5') Recreation
        .libraryTabItem(data-tab='6') Meetings & Events
    .photoTabListBody
        each item in data.page_photoLibrary.photoLibraryList
            ul.photoList.active(data-tab= "1")
                .wrapDiscover.wrapImgDetail
                    .container-xl
                        .contentItem
                            .listItem
                                .row.rowList.row3.showGalleryImgDetail
                                    each item in data.page_photoDetails.page_photoOverview.photos_list
                                        .colItem(data-src= item.images)
                                            .contentItem
                                                .wrapImg.wrapImagePhoto
                                                    .overlayImage
                                                    a.wrapImgResize.img16And9.imagePhoto(href='javascript:void(0);', title= item.title)
                                                        img(src= item.images, alt= item.title)
