            .contactSocial
                h4.contactTitle Social Media
                .contactSocialList
                    a.socialItem(href="javascript:void(0);") 
                        img.iconSocial(src='images/icon/icon-facebook.svg' alt='icon facebook')
                    a.socialItem(href="javascript:void(0);") 
                        img.iconSocial(src='images/icon/icon-twitter.svg' alt='icon facebook')
                    a.socialItem(href="javascript:void(0);") 
                        img.iconSocial(src='images/icon/icon-instagram.svg' alt='icon facebook')
                    a.socialItem(href="javascript:void(0);") 
                        img.iconSocial(src='images/icon/icon-youtube.svg' alt='icon facebook')
            .googleMapWrap
                iframe.googleMapImg(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3917.2710672790954!2d108.19058851524154!3d10.942884859090283!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x317685844e28b1e7%3A0xdef69de22952e45!2sVIPOL%20MUI%20NE%20HOTEL!5e0!3m2!1svi!2s!4v1650905671439!5m2!1svi!2s' width='800' height='250' style='border:0;' allowfullscreen='' loading='lazy' referrerpolicy='no-referrer-when-downgrade')
        .contactContentItem 
            h4 Leave Us A Comment
            .contactFormWrapper 
                form(autoComplete="off").contactForm
                    .contactFormItem
                        label(for="fullName") Full name:
                        input#fullName(type="text" placeholder="Please enter your full name...").formItem
                    .contactFormItem
                        label(for="phone") Phone:
                        input(type="text" placeholder="Please enter your phone...")#phone.formItem
                    .contactFormItem
                        label(for="email") Email address:
                        input(type="text" placeholder="Please enter your email...")#email.formItem
                    .contactFormItem
                        label(for="address") Address:
                        input(type="text" placeholder="Please enter your address...")#address.formItem
                    .contactFormItem
                        label(for="message") Your Message:
                        textarea#message(name="message" placeholder="Please enter your email..." rows="3").formItem 
                    button.button.btnFormContact SEND
// end contact
